{
  "name": "administration",
  "version": "2.1.1",
  "private": true,
  "dependencies": {
    "graphql": "15.8.0",
    "@apollo/client": "^3.5.9",
    "@blueprintjs/core": "^3.53.0",
    "@blueprintjs/datetime": "^3.23.20",
    "@blueprintjs/select": "^3.18.12",
    "@zxing/library": "^0.19.1",
    "core-js": "^3.21.1",
    "date-fns": "^2.28.0",
    "detect-browser": "^5.3.0",
    "fast-text-encoding": "^1.0.3",
    "jspdf": "^2.5.1",
    "long": "^5.2.0",
    "protobufjs": "^6.11.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-flip-move": "^3.0.4",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.3",
    "web-vitals": "^2.1.4",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/blob-stream": "^0.1.30",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-flip-move": "^2.9.12",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.23",
    "apollo": "^2.33.9",
    "run-script-os": "^1.1.6",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "generate-graphql": "apollo client:codegen --target=typescript --passthroughCustomScalars --customScalarsPrefix=GQL_",
    "generate-protobuf": "run-script-os",
    "generate-protobuf:linux": "mkdir -p src/generated && pbjs -t static-module -w es6 -o src/generated/compiled.js ../specs/card_activate_model.proto --force-long && pbts -o src/generated/compiled.d.ts src/generated/compiled.js",
    "generate-protobuf:win32": "mkdir src\\generated | pbjs -t static-module -w es6 -o src\\generated\\compiled.js ..\\specs\\card_activate_model.proto --force-long | pbts -o src\\generated\\compiled.d.ts src\\generated\\compiled.js",
    "postinstall": "npm run generate-protobuf"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": "src/generated/**"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 11"
    ]
  }
}
