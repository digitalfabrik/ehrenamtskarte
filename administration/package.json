{
  "name": "administration",
  "version": "2.1.1",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@blueprintjs/core": "^3.40.0",
    "@blueprintjs/datetime": "^3.21.0",
    "@blueprintjs/select": "^3.15.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/blob-stream": "^0.1.30",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-flip-move": "^2.9.12",
    "@zxing/library": "^0.18.3",
    "apollo": "^2.32.0",
    "apollo-boost": "^0.4.9",
    "core-js": "^3.8.3",
    "date-fns": "^2.16.1",
    "detect-browser": "^5.2.0",
    "fast-text-encoding": "^1.0.3",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "jspdf": "^2.2.0",
    "long": "^4.0.0",
    "protobufjs": "^6.10.2",
    "react": "^17.0.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^17.0.1",
    "react-flip-move": "^3.0.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "run-script-os": "^1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "generate-graphql": "apollo client:codegen --target=typescript --passthroughCustomScalars --customScalarsPrefix=GQL_",
    "generate-protobuf": "run-script-os",
    "generate-protobuf:linux": "mkdir -p src/generated && pbjs -t static-module -w es6 -o src/generated/compiled.js ../specs/card_activate_model.proto --force-long && pbts -o src/generated/compiled.d.ts src/generated/compiled.js",
    "generate-protobuf:win32": "mkdir src\\generated | pbjs -t static-module -w es6 -o src\\generated\\compiled.js ..\\specs\\card_activate_model.proto --force-long | pbts -o src\\generated\\compiled.d.ts src\\generated\\compiled.js",
    "postinstall": "npm run generate-protobuf"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": "src/generated/**"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 11"
    ]
  }
}
