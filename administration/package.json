{
  "name": "administration",
  "version": "2.1.1",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@blueprintjs/core": "^4.16.3",
    "@blueprintjs/datetime": "^4.3.12",
    "@blueprintjs/popover2": "^1.13.3",
    "@blueprintjs/select": "^4.9.3",
    "@bufbuild/protobuf": "^1.0.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@zxing/library": "^0.19.2",
    "apollo-upload-client": "^17.0.0",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "localforage": "^1.10.0",
    "notistack": "^2.0.8",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-move": "^3.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^1.0.0",
    "@graphql-codegen/cli": "^3.2.0",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@typescript-eslint/parser": "^5.53.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/blob-stream": "^0.1.30",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.14.0",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "apollo": "^2.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "jasmine": "^4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "prettier": "^2.8.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "set -ae; REACT_APP_APPLICATION_COMMIT=$(./application_commit.sh); react-scripts start",
    "build": "set -ae; REACT_APP_APPLICATION_COMMIT=$(./application_commit.sh); react-scripts build",
    "test": "karma start --single-run",
    "test:watch": "karma start --autowatch",
    "eject": "react-scripts eject",
    "lint": "eslint . && prettier . --check",
    "format": "eslint . --fix && prettier . --write",
    "generate-graphql": "graphql-codegen --config graphql-codegen.yml",
    "generate-protobuf": "protoc -I ../specs --es_out=src/generated --es_opt=target=ts --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es ../specs/card.proto",
    "postinstall": "npm run generate-protobuf && npm run generate-graphql"
  },
  "eslintConfig": {
    "plugins": [
      "jasmine"
    ],
    "extends": [
      "eslint-config-prettier",
      "react-app",
      "plugin:jasmine/recommended"
    ],
    "ignorePatterns": [
      "src/generated/**",
      "build/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 11"
    ]
  }
}
